  (DATE'1993-01-01','Толстой','122.50','Севастопольские рассказы'),
  (DATE'1993-01-01','Тургенев','450.50','Дым'),
  (DATE'1993-01-01','Тургенев','630.50','Вешние воды'),
  (DATE'1993-01-01','Достоевский','330.50','Идиот'),
  (DATE'1993-01-01','Достоевский','430.50','Игрок')
;

CREATE TABLE publishers
(
	"id" serial,
	"name" varchar(200) not null,
	"city" varchar(100) not null

);


CREATE TABLE "books"
(	"id" SERIAL ,
	"year" DATE NOT NULL,
	"author" VARCHAR(100),
	"price" NUMERIC(6,2) NOT NULL,
	"title" VARCHAR(200) NOT NULL
);

INSERT INTO "books" ("year", "author", "price", "title") VALUES
  (DATE'1993-01-01','Толстой','637.50','Анна Каренина'),
  (DATE'1993-01-01','Толстой','237.50','Воскресение'),
